{
  "expo": {
    "name": "fashion-week-mobile",
    "slug": "fashion-week-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "fashionweekmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.fashionweekmobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.fashionweekmobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/kanit/700Bold/Kanit_700Bold.ttf"
          ]
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": "Allow $(fashionweekmobile) to access your microphone."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(fashionweekmobile) to access your photos.",
          "cameraPermission": "Allow $(fashionweekmobile) to access your camera."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "SUPABASE_ANON_KEY": "YOUR_SUPABASE_ANON_KEY_HERE"
    }
  }
}
